{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "QuestionsSettings": {
    "TheoryTestQuestionsCount": 20
  },
  "JwtSettings": {
    "SecurityAlgorithm": "HmacSha256",
    "ExpirationTimeInMinutes": 20
  },
  "LoginSettings": {
    "IsPersistent": true,
    "LockoutOnFailure": false
  },
  "EmailConfirmationLinkGeneratorSettings": {
    "UserIdParameterName": "userId",
    "TokenParameterName": "encodedToken"
  }
}
