{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "QuestionsSettings": {
    "TheoryTestQuestionsCount": 20
  },
  "JwtSettings": {
    "SecurityAlgorithm": "HmacSha256",
    "ExpirationTimeInMinutes": 20
  },
  "LoginSettings": {
    "IsPersistent": true,
    "LockoutOnFailure": false
  },
  "EmailLinkGeneratorSettings": {
    "UserIdParameterName": "userId",
    "TokenParameterName": "token",
    "NewEmailParameterName": "newEmail"
  },
  "ImageSettings": {
    "AllowedContentTypes": [
      "image/jpeg",
      "image/png",
      "image/gif",
      "image/bmp"
    ]
  }
}
